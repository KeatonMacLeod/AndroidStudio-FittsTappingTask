<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jeeves.fittstappingtask">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Presentation.Choose_Screen"></activity>
        <activity android:name=".Presentation.ExperimentInstructionsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Presentation.ThumbInstructionsActivity" />
        <activity android:name=".Presentation.BreakInstructionsActivity" />
        <activity android:name=".Presentation.IndexFingerInstructionsActivity" />
        <activity android:name=".Presentation.ClosingRemarksActivity" />
        <activity android:name=".Presentation.TappingActivity" />
    </application>

</manifest>